{
    "name": "content-alchemy-command",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "emulators": "firebase emulators:start --import=./.firebase/emulators --export-on-exit",
        "clean-emulators": "kill-port 9099 5001 8080 5000",
        "dev:all": "npm run clean-emulators && node finalize-save.js && concurrently \"npm run emulators\" \"npm run dev\"",
        "env:prod": "node scripts/switch-env.js prod",
        "env:dev": "node scripts/switch-env.js dev",
        "save": "node save-data.js",
        "finalize": "node finalize-save.js"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^8.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@radix-ui/react-alert-dialog": "^1.1.15",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-label": "^2.1.8",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tabs": "^1.1.13",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^3.3.1",
        "firebase": "^10.8.0",
        "lucide-react": "^0.344.0",
        "moment": "^2.30.1",
        "next-themes": "^0.4.6",
        "react": "^18.2.0",
        "react-day-picker": "^9.13.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.1",
        "recharts": "^3.7.0",
        "sonner": "^2.0.7",
        "tailwind-merge": "^2.6.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.56",
        "@types/react-dom": "^18.2.19",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.17",
        "concurrently": "^9.2.1",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "kill-port": "^2.0.1",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "vite": "^5.1.4"
    }
}